{
    "name": "super-kernel/di",
    "type": "library",
    "license": "MIT",
    "description": "The di for super kernel framework.",
    "keywords": [
        "php",
        "swoole",
        "super-kernel",
        "di",
        "container"
    ],
    "autoload": {
        "psr-4": {
            "SuperKernel\\Di\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SuperKernelTest\\Di\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.4",
        "super-kernel/contract": "*"
    },
    "require-dev": {
        "super-kernel/event-dispatcher": "@dev",
        "super-kernel/logger": "@dev",
        "super-kernel/framework": "@dev",
        "super-kernel/command": "@dev"
    },
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true
    }
}
