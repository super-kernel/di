{
    "name": "super-kernel/di",
    "type": "library",
    "license": "MIT",
    "description": "The di for super kernel framework.",
    "keywords": [
        "php",
        "swoole",
        "super-kernel",
        "di",
        "container"
    ],
    "autoload": {
        "psr-4": {
            "SuperKernel\\Di\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SuperKernelTest\\Di\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "ext-pcntl": "*",
        "php": ">=8.4",
        "super-kernel/contract": "*",
        "nikic/php-parser": "^5.7",
        "super-kernel/attribute": "dev-main"
    },
    "require-dev": {
        "super-kernel/event-dispatcher": "@dev",
        "super-kernel/logger": "@dev",
        "super-kernel/framework": "@dev",
        "super-kernel/command": "@dev"
    },
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true
    },
    "repositories": [
        {
            "type": "path",
            "url": "../attribute"
        }
    ]
}
