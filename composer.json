{
    "name": "super-kernel/di",
    "type": "library",
    "license": "MIT",
    "description": "The di for super kernel framework.",
    "keywords": [
        "php",
        "swoole",
        "super-kernel",
        "di",
        "container"
    ],
    "autoload": {
        "psr-4": {
            "SuperKernel\\Di\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SuperKernelTest\\Di\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.4",
        "super-kernel/attribute": "1.1.*",
        "super-kernel/contract": "1.1.*",
        "symfony/filesystem": "^7.3"
    },
    "authors": [
        {
            "name": "wheakerd",
            "email": "wheakerd@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "super-kernel/event-dispatcher": "^1.0",
        "super-kernel/logger": "^1.0",
        "super-kernel/framework": "^1.0",
        "super-kernel/command": "^1.0"
    }
}
